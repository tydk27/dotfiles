[core]
    editor = vim -c 'set fenc=utf8'
    pager =
    excludesfile = /home/hoge/.gitignore
[user]
    name = hoge
    email = hoge@hoge
[color]
    ui = auto
[alias]
    co = checkout
    cm = commit
    st = status
    br = branch -va
    fe = fetch --prune
    pu = pull --prune
    lo = log --graph --name-status --pretty=format:'%C(blue)%h %C(green)%an %Creset%s %C(yellow)%d%Creset'
